<div class="form">
  <%= form_with(model: @image, local: true) do |form| %>
    <div class="image_new upper_image_new">
      <%= form.label :upper_image, {class: 'image_box upper_image_box'} %>
      <div class="image_preview upper_image_preview">SHIRTの画像を選択してください</div>
        <%= form.file_field :upper_image, {class: "select_imagefile select_upper_imagefile"}%>
    </div>
    <div class="image_new down_image_new">
      <%= form.label :down_image, {class: 'image_box down_image_box'} %>
      <div class="image_preview pants_image_preview">PANTSの画像を選択してください</div>
        <%= form.file_field :down_image, {class: "select_imagefile select_down_imagefile"}  %>
    </div>
    <div class="new_submit_btn">
      <%= form.submit "更新する" %>
    </div>
  <%end%>
</div>>